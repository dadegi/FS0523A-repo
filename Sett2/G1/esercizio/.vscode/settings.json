{
    "angular-schematics.schematicsDefaultOptions": {
        "angular-component": {
            "skipChangeDetectionOnPush": true,
            "ngOnInit": true
        },
        "angular-*": {
            "externalTemplate": true,
            "style": "scss"
        },
        "angular-page": {
            "skipChangeDetectionOnPush": true,
            "skipNgOnInit": false
        }
    },
    "angular-schematics.localSchematicsPrefix": "myComp",
    "angular-schematics.angularForceNgModules": true
}
